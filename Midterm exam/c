 #include<stdio.h>
 #include<conio.h>
 int main(void)
 {
    long int i,n=5,fac=1;
    i=1;
    do
    {
       fac*=i;
       i++;
    }
    while(i<=n);
 
    printf("Factorial = %ld\n",fac);
    getch();
    }
#include <stdio.h>
long int m(int n);

int main()
{
   int n=5;
 
   printf("Factorial of %d = %ld", n, m(n));
   return 0;
}
long int m(int n)
{
   if (n >= 1)
       return n*m(n-1);
   else
       return 1;
}
