#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf("Hello 偉大的恩師!\n");   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n");   /* 印出Hello Taiwan! 字串 */   
   
   system("pause");
   return 0;
   printf("Hello World!\n");
}
Window+shift+s局部截圖

#include <stdio.h> 
#include <stdlib.h> 
printme()
{ 
   printf("Hello 我親愛偉大的恩師!\n");
   printf("Hello 世界\n");
}
printtree()
{  
   printf("      \n");
   printf("      \n");
   printf("     *      \n");
   printf("    ***     \n");
   printf("   *****    \n");
   printf("  *******   \n");
   printf("**********  \n");
   printf("    ***     \n");
   printf("    ***     \n");
}
int main(void) 
{
   printf("我是4070E001李建輝\n");
   printme();
   printtree();
   system("pause");
   return  0;
}
